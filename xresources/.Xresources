URxvt.depth: 32
! value in [] is for opacity, value after is for bg color
URxvt.background: [85]#000000
URxvt.internalBorder: 10

! URxvt.font: xft:monospace:size=11
URxvt.font: xft:SauceCodePro Nerd Font Mono:pixelsize=19
/* URxvt.font: xft:Fira Code Nerd Font Complete Mono:pixelsize=19 *1/ */

URxvt*letterSpace: 0

URxvt.scrollBar: false
URxvt.cursorColor: orange

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false

URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

!!Extensions
!Urxvt.perl-lib: ~/.config/urxvt/ext/
!URxvt.perl-ext-common: default,matcher,resize-font,keyboard-select,url-select
!!! resize-font
!URxvt.keysym.C-minus:  font-size:decrease
!URxvt.keysym.C-equal:  font-size:increase
!!! keyboard-select
!URxvt.keysym.M-Escape: perl:keyboard-select:activate
!!! matcher
!URxvt.url-launcher: /usr/bin/xdg-open
!URxvt.matcher.button: 1
!URxvt.matcher.rend.0: Uline Bold fg5
!!! url-select
!!! URxvt.keysym.C-u:  url-select:select_next
!URxvt.url-select.launcher: /usr/bin/xdg-open
!URxvt.url-select.underline: true
